      <div class="card-body align-items-center">
        <h3 class="card-icon-title">
          {{ module.title }}
        </h3>
        <form class="card-form">
          <div class="card-form-step form-inline active">
            <div class="form-container-underline">
              <input type="text" class="form-control form-control-underline" placeholder="{{ module.placeholder }}"/>
              <div class="form-control-decor"></div>
            </div>
            <button id="btnNext" class="btn btn-white">{{ module.buttonText }}</button>
          </div>
          <div class="card-form-step form-inline">
            <div class="form-container-underline">
              <input type="text" class="form-control form-control-underline" placeholder="Name"/>
              <div class="form-control-decor"></div>
            </div>
            <div class="form-container-underline">
              <input type="email" class="form-control form-control-underline" placeholder="Email"/>
              <div class="form-control-decor"></div>
            </div>
            <button class="btn btn-white">Submit</button>
          </div>
        </form>
