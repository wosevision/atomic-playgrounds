<div class="row card-container card-container-stacked card-container-featured">
  {% for card in module.cardsFeature %}
    {% if card.align !== "right" %}
      <div class="col-lg-6 d-flex">
        {% include "modules/card.njk" %}
      </div>
    {% endif %}
  {% endfor %}
  <div class="col-lg-6">
    <div class="row">
      {% for card in module.cards %}
        <div class="col-sm-6 d-flex">
          {% include "modules/card.njk" %}
        </div>
      {% endfor %}
    </div>
  </div>
  {% for card in module.cardsFeature %}
    {% if card.align === "right" %}
      <div class="col-lg-6 d-flex">
        {% include "modules/card.njk" %}
      </div>
    {% endif %}
  {% endfor %}
</div>

{% if module.buttonText %}
<div class="row mt-5">
  <div class="col-12 text-center">
    <a href="{{ module.buttonLink }}" class="btn btn-primary">
      {{ module.buttonText }}
    </a>
  </div>
</div>
{% endif %}

