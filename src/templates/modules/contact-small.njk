<div class="row card-container-icon">
	<div class="col-12">
    <div class="card bg-primary border-dashed">
      <span class="card-icon fa fa-{{ module.icon }}">?</span>
      <div class="card-body">
        <h3 class="card-icon-title">
          {{ module.title }}
        </h3>
        <form class="card-form">
          <div class="d-flex">
            <input type="text" class="form-control" placeholder="{{ module.placeholder }}"/>
            <button class="btn btn-white">{{ module.buttonText }}</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
